<?xml version="1.0"?>
<!DOCTYPE package-info SYSTEM "http://www.simplemachines.org/xml/package-info">
<package-info xmlns="http://www.simplemachines.org/xml/package-info" xmlns:smf="http://www.simplemachines.org/">
	<id>dougiefresh:SearchSentPMs</id>
	<name>Search Sent PMs</name>
	<version>1.3</version>
	<type>modification</type>

	<install for="2.0 - 2.0.99">
		<readme type="file" parsebbc="true">readme.txt</readme>
		<modification>install.xml</modification>
		<modification type="file">language_english.xml</modification>
		<modification type="file">language_english-utf8.xml</modification>
		<modification type="file">language_english_british.xml</modification>
		<modification type="file">language_english_british-utf8.xml</modification>
		<modification type="file">language_portuguese_pt.xml</modification>
		<modification type="file">language_portuguese_pt-utf8.xml</modification>
	</install>
	<uninstall for="2.0 - 2.0.99">
		<modification type="file" reverse="true">install.xml</modification>
		<modification type="file" reverse="true">language_english.xml</modification>
		<modification type="file" reverse="true">language_english-utf8.xml</modification>
		<modification type="file" reverse="true">language_english_british.xml</modification>
		<modification type="file" reverse="true">language_english_british-utf8.xml</modification>
		<modification type="file" reverse="true">language_portuguese_pt.xml</modification>
		<modification type="file" reverse="true">language_portuguese_pt-utf8.xml</modification>
	</uninstall>
</package-info>
